<Window x:Class="KUMF5H_HFT_2021221.FrontendWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KUMF5H_HFT_2021221.FrontendWPF"
           xmlns:local1="clr-namespace:KUMF5H_HFT_2021221.FrontendWPF.VM"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded" >
<Window.DataContext>
        <local1:MainWindowViewModel/>
</Window.DataContext>
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button Command="{Binding ChangeToProducerCommand}" Content="ChangeToProducerCommand"/>
            <Button Command="{Binding ChangeToMedicineCommand}" Content="ChangeToMedicineCommand"/>
            <Button Command="{Binding ChangeToPatientCommand}" Content="ChangeToPatientCommand"/>
           
        </StackPanel>
    </Grid>
</Window>
